      <xs:element name="제품군">
      <xs:complexType>
			<xs:sequence>
				 <xs:element name="제품">
					<xs:complexType>
						 <xs:sequence>
							<xs:element name="제조사" type="xs:string"/>
							<xs:element name="제품명" type="xs:string"/>
							<xs:element name="제품가격" type="xs:integer"/>
							<xs:element name="할인율" type="xs:integer"/>
							<xs:element name="제품코드" type="xs:string"/>
